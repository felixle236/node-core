{
    "check-coverage": true,
    "lines": 80,
    "watermarks": {
        "lines": [80, 90],
        "functions": [80, 90],
        "branches": [80, 90],
        "statements": [80, 90]
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/core/shared/test/**/*.ts",
        "**/test/**"
    ],
    "extension": [
        ".ts"
    ],
    "require": [
        "ts-node/register",
        "tsconfig-paths/register"
    ],
    "reporter": [
        "text",
        "html"
    ],
    "instrument": true,
    "all": false,
    "cache": false
}