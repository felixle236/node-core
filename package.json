        "reflect-metadata": "^0.1.13",
        "routing-controllers": "^0.8.1",
        "sib-api-v3-sdk": "^8.1.0",
        "socket.io": "^3.0.5",
        "socket.io-emitter": "^3.2.0",
        "socket.io-redis": "^6.0.1",
        "stripe": "^8.130.0",
        "twilio": "^3.54.2",
