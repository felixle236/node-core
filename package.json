        "@types/express": "^4.17.8",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mailgen": "^2.0.3",
        "@types/mime-types": "^2.1.0",
        "@types/minio": "^7.0.6",
        "@types/mocha": "^8.0.3",
        "@types/multer": "^1.4.4",
