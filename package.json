        "@types/express": "^4.17.9",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mailgen": "^2.0.3",
        "@types/mailgun-js": "^0.22.11",
        "@types/mime-types": "^2.1.0",
        "@types/minio": "^7.0.7",
        "@types/mocha": "^8.2.0",
        "@types/multer": "^1.4.5",
