{
    "name": "node-core",
    "version": "2.0.38",
    "description": "The NodeJS framework is built with Clean Architecture, using NodeJS, Typescript, ExpressJS, TypeORM, PostgreSQL, Redis, etc... Easy to expand and maintain.",
    "license": "Felix Le",
    "readmeFilename": "README.md",
    "scripts": {
        "cache:clear": "./node_modules/typeorm/cli.js cache:clear",
        "migration:generate": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:generate -n",
        "migration:run": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:revert",
        "generate:module": "NODE_ENV=development node ./module-generator/generate.js",
        "lint": "eslint --ext .ts --ext .js ./src",
        "build": "npm run lint && grunt clean && grunt sync && tsc",
        "test": "npm run build && nyc mocha --exit ./src/**/*.test.ts",
        "dev": "NODE_ENV=development nodemon --exec ts-node src/app.ts",
        "start": "NODE_ENV=production node ./dist/app.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run build"
        }
    },
    "author": {
        "name": "Felix Le",
        "email": "felix.le.236@gmail.com"
    },
    "homepage": "https://github.com/felixle236/node-core#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/felixle236/node-core.git"
    },
    "bugs": {
        "url": "https://github.com/felixle236/node-core/issues"
    },
    "keywords": [
        "nodejs",
        "node core",
        "node service",
        "node api",
        "typescript",
        "clean architecture"
    ],
    "dependencies": {
        "@google-cloud/storage": "^5.7.1",
        "aws-sdk": "^2.824.0",
        "body-parser": "^1.19.0",
        "child-process-promise": "^2.2.1",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "mailgen": "^2.0.14",
        "mailgun-js": "^0.22.0",
        "mime-types": "^2.1.28",
        "minio": "^7.0.18",
        "multer": "^1.4.2",
        "node-pushnotifications": "^1.6.0",
        "nodemailer": "^6.4.17",
        "pg": "^8.5.1",
