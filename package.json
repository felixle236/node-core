        "@types/socket.io-client": "^1.4.34",
        "@types/socket.io-emitter": "^3.1.0",
        "@types/socket.io-redis": "^1.0.26",
        "@types/stripe": "^8.0.417",
        "@types/twilio": "^2.11.0",
        "@types/uuid": "^8.3.0",
        "@types/validator": "^13.1.3",
        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@typescript-eslint/parser": "^4.12.0",
        "chai": "^4.2.0",
        "eslint": "^7.17.0",
