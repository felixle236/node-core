{
    "name": "node-core",
    "version": "2.1.0",
    "description": "The NodeJS framework is built with Clean Architecture, using NodeJS, Typescript, ExpressJS, TypeORM, PostgreSQL, Redis, etc... Easy to expand and maintain.",
    "license": "Felix Le",
    "readmeFilename": "README.md",
    "scripts": {
        "postinstall": "husky install",
        "generate:module": "node ./module-generator/generate.js",
        "generate:usecase": "node ./module-generator/generate-usecase.js",
        "typeorm:cli": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js -f src/configs/DbConfig",
        "cache:clear": "npm run typeorm:cli -- cache:clear",
        "migration:generate": "npm run typeorm:cli -- migration:generate -n",
        "migration:run": "npm run typeorm:cli -- migration:run",
        "migration:revert": "npm run typeorm:cli -- migration:revert",
        "lint": "eslint --ext .ts --ext .js ./src",
        "build": "npm run lint && grunt clean && grunt sync && tsc",
        "test": "npm run build && nyc mocha --exit ./src/**/*.test.ts",
        "dev": "NODE_ENV=development nodemon --exec ts-node src/app.ts",
        "start": "NODE_ENV=production node ./dist/app.js"
    },
    "author": {
        "name": "Felix Le",
        "email": "felix.le.236@gmail.com"
    },
    "homepage": "https://github.com/felixle236/node-core#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/felixle236/node-core.git"
    },
    "bugs": {
        "url": "https://github.com/felixle236/node-core/issues"
    },
    "keywords": [
        "nodejs",
        "node core",
        "node service",
        "node api",
        "typescript",
        "clean architecture"
    ],
