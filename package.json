{
    "name": "node-core",
    "version": "2.0.8",
    "description": "The NodeJS framework is built with Clean Architecture, using NodeJS, Typescript, ExpressJS, TypeORM, PostgreSQL, Redis, etc... Easy to expand and maintain.",
    "license": "Felix Le",
    "readmeFilename": "README.md",
    "scripts": {
        "cache:clear": "./node_modules/typeorm/cli.js cache:clear",
        "migration:generate": "NODE_ENV=development ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -n",
        "migration:run": "NODE_ENV=development ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "NODE_ENV=development ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert",
        "generate:module": "NODE_ENV=development node ./module-generator/generate.js",
        "lint": "./node_modules/.bin/eslint --ext .ts --ext .js ./src",
        "build": "npm run lint && ./node_modules/.bin/grunt clean && ./node_modules/.bin/grunt sync && ./node_modules/.bin/tsc",
        "test": "npm run build && ./node_modules/.bin/nyc ./node_modules/.bin/mocha --exit ./src/**/*.test.ts",
        "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --exec './node_modules/.bin/ts-node' src/app.ts",
        "start": "NODE_ENV=production node ./dist/app.js"
    },
    "author": {
        "name": "Felix Le",
        "email": "felix.le.236@gmail.com"
    },
    "homepage": "https://github.com/felixle236/node-core#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/felixle236/node-core.git"
    },
    "bugs": {
        "url": "https://github.com/felixle236/node-core/issues"
    },
    "keywords": [
        "nodejs",
        "node core",
        "node service",
        "node api",
        "typescript",
        "clean architecture"
    ],
    "dependencies": {
        "aws-sdk": "^2.771.0",
        "body-parser": "^1.19.0",
        "child-process-promise": "^2.2.1",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "mailgen": "^2.0.14",
        "mime-types": "^2.1.27",
        "minio": "^7.0.16",
        "multer": "^1.4.2",
        "node-pushnotifications": "^1.4.2",
        "nodemailer": "^6.4.14",
        "pg": "^8.4.1",
        "redis": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "routing-controllers": "^0.8.1",
        "sib-api-v3-sdk": "^8.0.1",
        "socket-controllers": "0.0.5",
        "socket.io": "^2.3.0",
        "socket.io-emitter": "^3.1.1",
        "socket.io-redis": "^5.4.0",
        "stripe": "^8.111.0",
        "twilio": "^3.49.4",
        "typedi": "^0.8.0",
