{
    "name": "node-core",
    "version": "1.0.4",
    "description": "The NodeJS framework is built with Clean Architecture, using NodeJS, Typescript, ExpressJS, TypeORM, PostgreSQL, Redis, etc... Easy to expand and maintain.",
    "license": "Felix Le",
    "readmeFilename": "README.md",
    "scripts": {
        "cache:clear": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js cache:clear",
        "migration:generate": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -n",
        "migration:run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert",
        "generate:module": "node ./src/resources/source-templates/loader.js",
        "generate:claim": "./node_modules/.bin/grunt exec:generate_claim",
        "lint": "./node_modules/.bin/eslint --ext .ts --ext .js ./src",
        "build": "npm run lint && ./node_modules/.bin/grunt clean && ./node_modules/.bin/grunt sync && ./node_modules/.bin/tsc",
        "test": "npm run build && ./node_modules/.bin/nyc ./node_modules/.bin/mocha --exit --file ./src/test/**/*.spec.ts",
        "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --exec './node_modules/.bin/ts-node' src/app.ts",
        "start": "node ./dist/app.js"
    },
    "author": {
        "name": "Felix Le",
        "email": "felix.le.236@gmail.com"
    },
    "homepage": "https://github.com/felixle236/node-core#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/felixle236/node-core.git"
    },
    "bugs": {
        "url": "https://github.com/felixle236/node-core/issues"
    },
    "keywords": [
        "nodejs",
        "node core",
        "node service",
        "node api",
        "typescript",
        "clean architecture"
    ],
    "dependencies": {
        "aws-sdk": "^2.632.0",
        "body-parser": "^1.19.0",
        "child-process-promise": "^2.2.1",
        "class-validator": "^0.11.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "ejs": "^3.0.1",
        "express": "^4.17.1",
        "file-type": "^14.1.3",
        "jsonwebtoken": "^8.5.1",
        "mailgen": "^2.0.10",
        "minio": "^7.0.14",
        "multer": "^1.4.2",
        "node-pushnotifications": "^1.2.1",
        "nodemailer": "^6.4.4",
        "pg": "^7.18.2",
        "redis": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.2",
        "routing-controllers": "^0.8.0",
        "sib-api-v3-sdk": "^7.1.4",
        "socket-controllers": "0.0.5",
        "socket.io": "^2.3.0",
        "socket.io-emitter": "^3.1.1",
        "socket.io-redis": "^5.2.0",
        "stripe": "^8.25.0",
        "typedi": "^0.8.0",
        "typeorm": "^0.2.24",
        "typeorm-typedi-extensions": "^0.2.3"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.0",
        "@types/chai": "^4.2.10",
        "@types/compression": "1.7.0",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.3",
        "@types/file-type": "^10.9.1",
        "@types/jsonwebtoken": "^8.3.8",
        "@types/mailgen": "^2.0.3",
        "@types/minio": "^7.0.5",
        "@types/mocha": "^7.0.2",
        "@types/multer": "^1.4.2",
        "@types/node": "^13.7.7",
        "@types/node-pushnotifications": "^1.0.3",
        "@types/nodemailer": "^6.4.0",
        "@types/redis": "^2.8.16",
        "@types/request-promise": "^4.1.45",
        "@types/sinon": "^7.5.2",
        "@types/socket.io": "^2.1.4",
        "@types/socket.io-client": "^1.4.32",
        "@types/socket.io-emitter": "^3.1.0",
        "@types/socket.io-redis": "^1.0.25",
        "@types/stripe": "^7.13.22",
        "@types/validator": "^12.0.1",
        "@typescript-eslint/eslint-plugin": "^2.22.0",
        "@typescript-eslint/parser": "^2.22.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "grunt": "^1.0.4",
        "grunt-contrib-clean": "^2.0.0",
        "grunt-exec": "^3.0.0",
        "grunt-sync": "^0.8.2",
        "mocha": "^7.1.0",
        "nodemon": "^2.0.2",
        "nyc": "^15.0.0",
        "redis-mock": "^0.49.0",
        "request-promise": "^4.2.5",
        "sinon": "^9.0.0",
        "socket.io-mock": "^1.2.4",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.3"
    }
}
