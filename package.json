        "@types/socket.io": "^2.1.11",
        "@types/socket.io-client": "^1.4.34",
        "@types/socket.io-emitter": "^3.1.0",
        "@types/socket.io-redis": "^1.0.26",
        "@types/stripe": "^7.13.24",
        "@types/twilio": "^2.11.0",
        "@types/uuid": "^8.3.0",
        "@types/validator": "^13.1.0",
